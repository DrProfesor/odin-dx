package dx

_D3D_SHADER_MACRO :: struct {
    Name: LPCSTR,
    Definition: LPCSTR,
}

ID3D10Blob :: struct {
    using vtbl: ^ID3D10BlobVtbl,
}

ID3D10BlobVtbl :: struct {
    QueryInterface : proc(This: ^ID3D10Blob, riid: REFIID, ppvObject: ^rawptr) -> HRESULT,
    AddRef : proc(This: ^ID3D10Blob) -> ULONG,
    Release : proc(This: ^ID3D10Blob) -> ULONG,
    GetBufferPointer : proc(This: ^ID3D10Blob) -> LPVOID,
    GetBufferSize : proc(This: ^ID3D10Blob) -> SIZE_T,
}

ID3DDestructionNotifier :: struct {
    using vtbl: ^ID3DDestructionNotifierVtbl,
}

ID3DDestructionNotifierVtbl :: struct {
    QueryInterface : proc(This: ^ID3DDestructionNotifier, riid: REFIID, ppvObject: ^rawptr) -> HRESULT,
    AddRef : proc(This: ^ID3DDestructionNotifier) -> ULONG,
    Release : proc(This: ^ID3DDestructionNotifier) -> ULONG,
    RegisterDestructionCallback : proc(This: ^ID3DDestructionNotifier, callbackFn: PFN_DESTRUCTION_CALLBACK, pData: rawptr, pCallbackID: ^UINT) -> HRESULT,
    UnregisterDestructionCallback : proc(This: ^ID3DDestructionNotifier, callbackID: UINT) -> HRESULT,
}

